<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="issuepickerLayout" maxHeight="600.0" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Label fx:id="issuepickerTitle" maxWidth="400.0" style="-fx-font-size: 125%;" text="Selected Issue(s)" />
      <FlowPane fx:id="selectedIssues" hgap="5.0" vgap="5.0">
         <padding>
            <Insets bottom="10.0" top="20.0" />
         </padding>
      </FlowPane>
      <TextField fx:id="issuepickerQueryField" prefColumnCount="30" />
      <ScrollPane fx:id="issuepickerScrollPane" fitToWidth="true" maxHeight="500.0" prefHeight="500.0">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
         <content>
            <VBox fx:id="suggestedIssues" spacing="10.0">
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</VBox>
